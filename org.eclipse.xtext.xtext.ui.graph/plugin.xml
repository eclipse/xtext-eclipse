<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.xtext.viewCategory"
            class="org.eclipse.xtext.xtext.ui.graph.bundle.RailroadViewExecutableExtensionRegistry:org.eclipse.xtext.xtext.ui.graph.RailroadView"
